# Docker development environment
NODE_ENV=development

# Database (these will be overridden by docker-compose.yml)
DB_HOST=mysql
DB_NAME=starwars_pokemon_db
DB_USER=api_user
DB_PASSWORD=api_password
DB_PORT=3306

# Redis (these will be overridden by docker-compose.yml)
REDIS_HOST=redis
REDIS_PORT=6379

# External APIs - CHANGE THESE AS NEEDED
SWAPI_BASE_URL=https://swapi.info/api
POKEAPI_BASE_URL=https://pokeapi.co/api/v2

# Application Configuration
CACHE_TTL_MINUTES=30
API_RATE_LIMIT_RPM=60
LOG_LEVEL=debug

# AWS Configuration (mocked for development)
AWS_REGION=us-east-1
COGNITO_USER_POOL_ID=local-dev-pool