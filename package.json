{
  "name": "star-wars-pokemon-api",
  "version": "1.0.0",
  "description": "Star Wars Pokemon Fusion API - Serverless Backend Challenge",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "serverless deploy",
    "test": "jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest src/tests/unit",
    "test:integration": "NODE_ENV=test jest src/tests/integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "dev": "serverless offline --config serverless.local.yml",
    "dev:local": "npm run setup:local && npm run dev",
    "setup:local": "scripts/setup-local-dev.bat",
    "migration:run": "node -r ts-node/register src/infrastructure/adapters/database/mysql/migrations/run.ts",
    "seed:run": "node -r ts-node/register src/infrastructure/adapters/database/mysql/seeds/run.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.864.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.864.0",
    "@aws-sdk/client-dynamodb": "^3.864.0",
    "@aws-sdk/client-rds": "^3.456.0",
    "@aws-sdk/util-dynamodb": "^3.864.0",
    "aws-xray-sdk-core": "^3.6.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.2",
    "dotenv": "^17.2.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "redis": "^4.6.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.129",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.3.0",
    "serverless-plugin-typescript": "^2.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
